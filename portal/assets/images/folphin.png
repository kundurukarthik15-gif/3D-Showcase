                $('#destination-details').height('150');
                $('.read-more').show();
                $('.read-less').hide();
            }
        })
        
        $(document).ready(function() {
              $('.dropdown-hover').hover(function() {
                $(this).addClass('show');
                $(this).find('.dropdown-menu').addClass('show');
