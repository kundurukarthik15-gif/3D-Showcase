<script>
    $(document).on('click', '.btn-read-more', function(e) {
        e.preventDefault();
        $('.destination-description').addClass('expanded');
        $('.btn-read-more').hide();
        $('.btn-read-less').show();
    })
    $(document).on('click', '.btn-read-less', function(e) {
        $('.destination-description').removeClass('expanded');
        $('.btn-read-more').show();
        $('.btn-read-less').hide();
    })
    
    $(document).on('click', '.btn-review-more', function(e) {
        e.preventDefault();
        var review_box = $(this).closest('.review-box');
        var review_text = review_box.find('.review-description');
        var review = review_text.attr('data-review');
        review_text.html(review)
        review_box.find('.btn-review-more,.btn-review-less').hide()
        
    })
</script>

<script>
    $(document).click(function() {
        $('.search-results').hide();
    });
    let checkSearchInterval
    $(document).on('submit', '#universal-search-form', function(e) {
        e.preventDefault()
    })
    
    $(document).on('input', '#universal-search', function(e) {
        console.log(e)
        e.preventDefault()
        var q = e.target.value
        $('.search-results-items').html('Loading results, please wait..')
        
        if(checkSearchInterval) {
          clearInterval(checkSearchInterval);
        }
        checkSearchInterval = setTimeout(function() {
            if(q.length >= 1) {
                $('.search-results').show();
                $('.search-query').html(`Search results for <b class="text-black"><i>${q}</i></b>`)
                
                $.ajax({
                    url : `https://travelsetu.com/guide/search-places?q=${q}`,
                    method: 'get',
                    success: function(data) {
                        if(data.list && data.list.length >= 1) {
                            var content = '';
                            data.list.map((item) => {
                                content += `
                                    <a href="${item.url}" class="list-group-item list-group-item-action d-flex">
                                        <div class="me-3">
                                            <img src="${item.photo ? item.photo.thumbnail : 'https://travelsetu.com/guide/uploads/holiday.jpg'}" width="50" height="50" class="rounded object-fit-cover" />
                                        </div>
                                        <div>
                                            <div class="fw-bold">${item.title}${item.parent ? '<span class="text-muted fw-normal">, '+item.parent.title + '</span>' : ''}</div>
                                            ${item.info_place_type ? '<div>' +  item.info_place_type + '</div>' : '' }
                                        </div>
                                    </a>
                                `
                            })
                            console.log(content)
                            $('.search-results-items').html(content)
                        } else {
                            $('.search-results-items').html('No results found!')
                        }
                    }
                })
            } else {
                $('.search-results').hide();
            }
        }, 500)
    })
</script>
<script>
    const _lp = localStorage.getItem("_lp_url");
    if(!_lp) {
        const url = window.location.href
        const timestamp = Date.now()
        const referrer = document.referrer
        var data = {
            url : url,
            timestamp : timestamp,
            referrer : referrer
        }
        localStorage.setItem("_lp", JSON.stringify(data));
    }
</script>
